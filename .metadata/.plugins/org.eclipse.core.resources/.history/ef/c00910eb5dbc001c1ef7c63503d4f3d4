#include <stdio.h>
#include <stdlib.h>
#include "funciones.h"

int main(void) {

	setbuf(stdout,NULL);

	//declarando variables.
	int km = 0;
	float precioAerolineas = 0;
	float precioLatam = 0;

	printf("Agencia De Vuelo \n");


	km = obtenerKm();
	precioAerolineas = obtenerAerolineas();
	precioLatam = obtenerLatam();

	printf("%i\n", km);
	printf("%f\n", precioAerolineas);
	printf("%f\n", precioLatam);

	return EXIT_SUCCESS;
}
