float descuentoTarjeta(float precio){

	float descuento;
	float resultado;

	printf("%f\n",precio);

	//aplicamos el 10% de descuento.
	descuento = precio * (10 / 100);

	resultado = precio - descuento;

	printf("%f\n",resultado);

	return resultado;

}


